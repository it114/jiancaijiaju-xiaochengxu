.container{
  width:100%;
  background: #F6F6F6;
}
.swiper{
  width:750rpx;
  height:750rpx;
  box-sizing: border-box;
}
.swiper swiper-item{
  width:100%;
  height:100%;
}
.swiper swiper-item navigator{
  width:100%;
  height:100%;
}
.swiper swiper-item navigator image{
  width: 100%;
  height:100%;
}
.tab_menu{
  width:100%;
  padding-left:32rpx;
  padding-right:32rpx;
  box-sizing:border-box;
  overflow: hidden;
  background: #ffffff;
}
.menu_item{
  width:25%;
  box-sizing:border-box;
  float:left;
  text-align: center;
}
.menu_item text{
  color:#333333;
  font-size:24rpx;
  margin-top:12rpx;
}
 .out_bg{
  width:100rpx;
  height:100rpx;
  margin:40rpx auto 0;
  padding-top:26rpx;
  border-radius: 50%;
  overflow: hidden;
  box-sizing: border-box;
}
.menu_01{
   background: linear-gradient(-180deg, #2EE0F9 39%, #3BBEFF 100%); 
}
.menu_02{
   background: linear-gradient(-180deg, #5DDF99 39%, #2FD5AE 100%); 
}
.menu_03{
   background: linear-gradient(-180deg, #FFA854 39%, #FE8F5E 100%); 
}
.menu_04{
   background: linear-gradient(-180deg,  #FFD300 39%, #FEB50F 100%); 
}
.menu_01 image{
  width:48rpx;
  height:48rpx;
}
.menu_02 image{
  width:48rpx;
  height:48rpx;
}
.menu_03 image{
  width:36rpx;
  height:48rpx;
}
.menu_04 image{
  width:46rpx;
  height:48rpx;
}
.activity{
  width:100%;
  padding:20px 16px 13px;
  box-sizing: border-box;
  overflow: hidden;
  background: #ffffff;  
}
.a_left,.a_right{
  width:48%;
  height:360rpx; 
  border-radius:4px;
  overflow: hidden;
  box-sizing: border-box;
  position:relative;  
}
.a_left image{
  position:absolute;
  top:0;
  left:0;
}
.a_right image{
  position: absolute;
  top:0;
  left:0;
}
.a_left image,.a_right image{
  width:100%;
  height:100%;
}
.a_left{
  float:left;
}
.a_right{
  float:right;
}
.btm{
  width:100%;
  height:120px;
  margin-top:10px;
}
.btm image{
  width:100%;
  height:100%;
}
.modelbg{
  background: rgba(0, 0, 0, .5);
  position: fixed;
  width: 100vw;
  height: 100vh;
  z-index: 111;
}
/* 自定义弹框 */  
.model{  
  background:#fff;  
  position:absolute;  
  top:32%;  
  z-index:10000;  
  width:80%;  
  left: 0;
  right: 0;
  margin: auto; 
  border-radius:20rpx;  
}  
.model .modelTitle{  
  width:100%;  
  height:90rpx;  
  text-align:center;  
  line-height:100rpx;  
  border-bottom:1px solid #ECECED;  
  color:#222;  
  font-size:38rpx;  
  font-family:“微软雅黑”;  
}  
.model .modelBody{  
  color:#B1B1B1;  
  width:91%;  
  height:110rpx;  
  padding:30rpx;  
  font-size:35rpx;  
  line-height:60rpx;  
  padding-left:30rpx;  
  padding-top:40rpx;  
}  
.model .btns{  
  position:relative;  
  overflow:hidden;  
  border-top:1px solid #ECECED;  
}  
.model .btns button{  
  width:100%;  
  display:inline-block;  
  padding:0rpx;  
  background:#fff;  
  /* opacity: 0; */  
}  
.model .btns button::after {  
  border: none;  
}  
.model .btns .agree {  
  color:#21B922;  
}  
.mask{  
  width:100%;  
  height:100%;  
  background:rgba(0, 0, 0, .5);  
  position:fixed;  
  top:0;
  z-index: 11;  
  /* background: red; */
}  
/**第一次加载**/
.notice{
width: 300px;
height: 150px;
position: fixed;
right: 15px;
top: 0;
}

.know{
width: 150px;
height: 60px;
position: fixed;
left: 30%;
top: 35%;
}

.container_activity{
  width:100%;
  height:100%;
  background: #fff;
  padding-bottom: 10px;
}
.activity_list{
  width:100%;
  height:196rpx;
  padding:12px 15px 0px;
  box-sizing: border-box;
  position:relative;
   margin-top: 6px;
}
.left_img{
  width:234rpx;
  height:172rpx;
  position:absolute;
}
.left_img image{
  width:100%;
  height:100%;
  border-radius:4px;
}
.right_info{
  width:100%;
  padding-left:83px;
  box-sizing:border-box;
}
.right_info .title{
  height:80rpx;
  font-size:28rpx;
  color:#333333;
  line-height:40rpx;
  overflow:hidden;  
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
}
.place,.time{
  font-size:24rpx;
  color:#666666;
   line-height:34rpx; 
}
.time{
  margin-top:14rpx;
}
.place{
  margin-top:10rpx;
}
.time image{
  width:28rpx;
  height:28rpx;
  vertical-align: middle;
  margin-top:-4rpx;
  margin-right:10rpx;
}
.place image{
  width:32rpx;
  height:32rpx; 
  vertical-align: middle;
  margin-top:-4rpx;  
  margin-right:10rpx;  
}
.envelopes_page{
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .6);
  z-index: 1;
}
.red-packet{
  width: 40px;
  height: 40px;
  z-index: 100;
  transition-property:transform,top;
  transform-origin:50% 50% 0;
  -webkit-transition-property:transform,top;
  -webkit-transform-origin:50% 50% 0;
}
.couponbg{
  width: 520rpx;
  height: 700rpx;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.couponbt{
  width: 470rpx;
  height: 80rpx;
  position: absolute;
  bottom: 20rpx;
  left: 0;
  right: 0;
  margin: auto;
}
.coupon_box{
  position: absolute;
  width: 430rpx;
  height: 390rpx;
  /* background: #fff; */
  top:210rpx;
  left: 0;
  right: 0;
  margin: auto;
}
.item-coupon-img{
  width: 88rpx;
  height: 88rpx;
}
.item-coupon-content{
  display: flex;
  margin-bottom: 40rpx;
}
.item-coupon-time{
  display: block;
  color: #666;
  font-size: 22rpx;
}
.item-coupon-title{
  width: 195rpx;
  font-size: 26rpx;
  color:#333;
  display: block;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  margin-bottom: 18rpx;
}

.coupon-money{
  display: block;
  color: #f6543f;
  font-size: 22rpx;
  margin-top: -10rpx;
}
.money-num{
  font-size: 40rpx;
}