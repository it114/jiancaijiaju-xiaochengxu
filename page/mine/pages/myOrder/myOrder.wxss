#top{  
  width: 100%;
  height: 44px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-flex-direction: row;
  flex-direction: row;
  line-height: 44px;
  border-top: 1px solid #f2f2f2;
  border-bottom: 1px solid #f2f2f2;
  position: fixed;  
  top: 0;  
  left: 0;
  z-index: 100000;
  background: #fff;
}
#top .item{
  height: 44px;
  font-size: 14px;
  text-align: center;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
#top .item view{
  width: 44px;
  margin: 0 auto;
}
.on{ color: #C4161E;  
    border-bottom:1rpx solid #C4161E;
}  
.list{
  position:relative;
  width: 100%;
  padding: 0 15px;
  box-sizing: border-box;
  background: #fff;
  padding-bottom: 12px;
  overflow: hidden;
}
.top_con{
  width: 100%;
  padding: 0 15px;
  box-sizing: border-box;
  height: 44px;
  line-height: 44px;
  overflow: hidden;
  background: #fff;
  margin-top: 10px;
  box-sizing: border-box;
}
.top_left{
  float: left;
  color: #333;
  font-size: 12px;
}
.top_right{
  float: right;
  color: #999;
  font-size: 12px;
}
.list .mid_con{
  position: relative;
}
.list .left_image{
  position: absolute;
  top: 0;
  left: 0;
  width: 110px;
  height: 90px;
  border-radius: 4px;
  overflow: hidden;
  float: left;
}
.list .right{
  position: relative;
  height: 90px;
  width: 100%;
  box-sizing: border-box;
  padding-left: 120px;
}
.list .title{
  text-overflow: -o-ellipsis-lastline;overflow: hidden;text-overflow: ellipsis;
  display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;font-size: 14px;color: #333;
  text-align: left;
  line-height: 20px;
}
.list .notice{
  color: #999;
  font-size: 12px;
  margin-top: 9px;
  line-height: 17px;
}
.list .price{
  color: #C4161E;
  font-size: 16px;
  line-height: 16px;
  margin-top: 9px;
}
.list .num{
  color: #333;
  font-size: 12px;
  margin-top: 4px;
  float: right;
}
.bottomButtom{
  padding: 12px 0;
  overflow: hidden;
  border-top: 1px solid #f2f2f2;
  background: #fff;
}
.bottomButtom view{
  float: left;
  color: #333;
  font-size: 14px;
  margin-left: 15px;
}
.bottomButtom text{
  color: #C4161E;
}
.bottomButtom .confirm{
  width: 52px;
  height: 22px;
  line-height: 22px;
  text-align: center;
   border: 1px solid #999;
  font-size: 12px;
  float: right;
  color: #333;
  border-radius: 4px;
  margin-right: 15px;
}
.bottomButtom .active{
  border: 1px solid #C4161E;
  color: #C4161E;
}
.bottomButtom .cancel{
  width: 48px;
  height: 22px;
  line-height: 22px;
  text-align: center;
  float: right;
  color: #333;
  font-size: 12px;
  border: 1px solid #999;
  border-radius: 4px;
}
scroll-view {
  width: 100%;
  background: #f2f2f2;
}
#loadMoreDiv{
  margin-top: 10px;
}