.container{
	width:100%;
	height:100%;
	padding-top:50rpx;
	box-sizing:border-box;
}
.logo{
	width:150rpx;
	height:150rpx;
	border-radius:100%;
	overflow: hidden;
	margin:0 auto;
	margin-bottom:100rpx;
}
.logo>image{
	width:100%;
	height:100%;
}
.btn{
	width:62.7%;
	height:88rpx;
	background:#C4161E;
	text-align:center;
	line-height:88rpx;
	font-size:32rpx;
	color:#ffffff;
	margin:40rpx auto 100rpx;
	border-radius:4px;
}