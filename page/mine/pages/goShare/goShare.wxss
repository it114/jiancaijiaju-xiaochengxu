.swiper{
  width: 100%;
}
.swiper image{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  padding:0 15px;
  height: 136px;
  box-sizing: border-box;
  position: relative;
}
.list{
  width: 100%;
  height: 136px;
  position:relative;
  margin-top: 12px;
}
.list .left{
  position: absolute;
  top: 0;
  left: 0;
  width: 136px;
  height: 136px;
  border-radius: 4px;
  overflow: hidden;
  float: left;
}
.list .right{
  position: relativ;
  width: 100%;
  box-sizing: border-box;
  padding-left: 146px;
}
.list .title{
  text-overflow: -o-ellipsis-lastline;overflow: hidden;text-overflow: ellipsis;
  display: -webkit-box;-webkit-line-clamp: 2;-webkit-box-orient: vertical;font-size: 14px;color: #333;
}
.list .notice{
  color: #999;
  font-size: 12px;
  margin-top: 10px;
}
.list .bottom_right{
  position: absolute;
  right: 0;
  bottom: 0;
  width: 84px;
  height: 36px;
  line-height: 36px;
  text-align: center;
  font-size: 14px;
  color: #fff;
  background: #C4161E;
  border-radius: 4px;
}
.list .bottom_left{
  position: absolute;
  left: 146px;
  bottom: 10px;
  color: #C4161E;
  font-size: 16px;
  line-height: 16px;
}