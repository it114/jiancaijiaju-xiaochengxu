 .list_container{
  position: relative;
  width: 92%;
  margin: 0 auto;
  margin-top: 10px;
  border-radius: 4px;
  height:124px;
  overflow: hidden;
}
.list{
  width: 100%;
  height:124px;
  background:#F18882;
  color: #fff;
  display:flex;
  display:-webkit-flex;
  align-items:center;/*垂直居中*/
  -webkit-align-items : center;/*垂直居中*/
}

.list .price{
  float: left;
  font-size: 28px;
  margin-left: 10px;
}
.swiper-box .list .price text{
  font-size: 18px;
}


.list .middle{
   /* float: left;  */
  font-size: 12px;
  width: 140px;
  margin: 0 auto;
  line-height: 20px;
}
.list .title{
  font-size: 14px;
}
.button{
  float: right;
  width: 58px;
  height: 24px;
  line-height: 24px;
  text-align: center;
  background: #fff;
  color: #C4161E;
  font-size: 12px;
  border-radius: 4px;
  margin-right: 15px;
}

#circle_left,#circle_right{
  display: block;
  position: absolute;
  top: 56px;
  width: 12px;
  height:12px;
  border-radius: 100%;
  background: #fff;
}
#circle_left{
  left: -6px;
}
#circle_right{
  right: -6px;
}
#loadMoreDiv{text-align: center;font-size: 12px;color:#999;padding-top: 10px;bottom: 10px;}
#loadMoreDiv text{
  vertical-align: 5px;
  margin-left: 2px;
}
#loadMoreDivimage{
	width: 20px;
  height: 20px;
	opacity: 0.6;
}
.notFound image{
  display: block;
  width: 187px;
  height: 128px;
  margin: 0 auto;
  padding-top: 100px;
}
.notFound text{
  display: block;
  width: 100%;
  text-align: center;
  margin-top: 10px;
  font-size: 14px;
  color: #333;
}